<%= field_set_tag( sortable_label( f.object.to_s ), class: 'nested-fields' ) do %>
  <%= f.input :name %>
  <%= f.input :position, as: :hidden %>
  <%= cocoon_fields f, :meeting_item_templates, insertable: true, ordered_by: :position %>
  <%= link_to_remove_association 'Remove Meeting Section Template', f %>
<% end %>

