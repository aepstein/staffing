<% if schedules.any? %>
  <%=content_tag :table, id: "schedules" do %>
    <%=content_tag :thead do %>
      <%=content_tag :tr do %>
        <%=content_tag :th, "Name" %>
      <% end %>
    <% end %>
    <%=content_tag :tbody do %>
      <%= render partial: 'schedules/schedule', collection: schedules %>
    <% end %>
  <% end %>
<% else %>
  <%=content_tag :p, "No schedules." %>
<% end %>

