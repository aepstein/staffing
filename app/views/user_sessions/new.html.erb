<h1>Login</h1>

<%= form_for @user_session, :url => user_session_path do |f| %>
  <%= render "shared/error_messages", :target => f.object %>

  <p>
  <%= f.label :net_id, 'Net' %><br />
  <%= f.text_field :net_id %>
  </p>

  <p>
  <%= f.label :password %><br />
  <%= f.password_field :password %>
  </p>

  <p>
  <%= f.check_box :remember_me %> <%= f.label :remember_me %>
  </p>

  <p>
  <%= f.submit "Login" %>
  </p>

<% end %>

